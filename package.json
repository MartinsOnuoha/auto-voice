{
  "name": "auto-voice",
  "version": "0.0.1",
  "description": "Automatic Invoices",
  "productName": "AutoVoice",
  "author": "MartinsOnuoha <martinsvictor.nd@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "test": "jest",
    "build": "quasar build",
    "test:unit": "jest --updateSnapshot",
    "test:e2e": "cross-env E2E_TEST=true start-test \"quasar dev\" http-get://localhost:8080 \"cypress open\"",
    "test:e2e:ci": "cross-env E2E_TEST=true start-test \"quasar dev\" http-get://localhost:8080 \"cypress run\"",
    "test:unit:ci": "jest --ci",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\""
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "apollo-boost": "^0.4.9",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "graphql": "^15.5.1",
    "magic-sdk": "^4.4.1",
    "quasar": "^2.0.0",
    "ts-jest": "^26.5.6",
    "vue-apollo": "^3.0.7",
    "vue-i18n": "^9.0.0-beta.0",
    "vuex": "^4.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.13.14",
    "@quasar/app": "^3.0.0",
    "@quasar/quasar-app-extension-testing": "^1.0.3",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^3.1.0",
    "@quasar/quasar-app-extension-testing-unit-jest": "^2.2.2",
    "@vue/test-utils": "^2.0.0-rc.9",
    "babel-jest": "^26.5.6",
    "dotenv": "^10.0.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-vue": "^7.0.0",
    "eslint-webpack-plugin": "^2.4.0",
    "jest": "^26.5.6",
    "vue-jest": "^5.0.0-alpha.10",
    "@vue/cli-plugin-unit-jest": "5.0.0-beta.2"
